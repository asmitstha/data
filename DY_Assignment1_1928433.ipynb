   "source": [
    "\n",
    "def ln(x):\n",
    "    total= 0\n",
    "    for n in range(1,30):\n",
    "        taylor_sum =   (((-1)**(n+1)*(x-1)**n)/n)\n",
    "        \n",
    "        total =(total + taylor_sum)\n",
    "        \n",
    "    return total\n",
    "\n"
   ]
  },
